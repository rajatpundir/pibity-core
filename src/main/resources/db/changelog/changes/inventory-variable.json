        "changes": [
          {
            "sqlFile": {
              "dbms": "postgresql",
              "encoding": "UTF-8",
              "endDelimiter": "\\nGO",
              "path": "test.sql",
              "relativeToChangelogFile": true,
              "splitStatements": true,
              "stripComments": true
            }
          }
        ]
      }
    },
    {
      "changeSet": {
        "id": "triggerforvariable",
        "author": "shubham.gupta",
        "changes": [
          {
            "sql": {
              "comment": "partition based on typeName",
              "dbms": "postgresql",
              "endDelimiter": "GO",
              "splitStatements": true,
              "sql": "CREATE TRIGGER variable_type_trigger\nBEFORE INSERT ON inventory.variable\nFOR EACH ROW EXECUTE PROCEDURE inventory.variable_type_function();",
              "stripComments": true
            }
          }
        ]
      }
    },
